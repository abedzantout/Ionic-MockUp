{
  "compilerOptions": {
	"allowSyntheticDefaultImports": true,
	"target": "es5",
	"module": "commonjs",
	"moduleResolution": "node",
	"declaration": true,
	"sourceMap": false,
	"emitDecoratorMetadata": true,
	"experimentalDecorators": true,
	"removeComments": false,
	"noImplicitAny": false,
	"lib": [
	  "dom",
	  "es2015"
	]
  },
  "exclude": [
	"node_modules"
  ],
  "compileOnSave": false,
  "atom": {
	"rewriteTsconfig": false
  }
}